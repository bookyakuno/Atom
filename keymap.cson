# ============================================================
'.tree-view':

  # ツリービュー 名称変更
  'ctrl-5': 'tree-view:move'


  # ============================================================
'.platform-darwin':






    'shift-alt-cmd-x': 'vim-mode-plus-user:move-to-previous-symbol'
    'shift-alt-cmd-c': 'vim-mode-plus-user:move-to-next-symbol'



    # 'shift-alt-cmd-c': 'symbols-view:go-to-declaration'
    # 'shift-alt-cmd-x': 'symbols-view:return-from-declaration'
    # 'shift-cmd-alt-R': 'symbols-view:toggle-project-symbols'





    # 'cmd-alt-tab':          'tree-view:toggle'
    'ctrl-cmd-r': 'tree-view:reveal-active-file'
    # 'ctrl-4': 'tree-view:toggle

    # ツリービューにフォーカス
    'ctrl-4': 'tree-view:toggle-focus'


    # ============================================================
'.platform-darwin, .platform-win32, .platform-linux':
  'shift-ctrl-g': 'go-to-line:toggle'


  # ============================================================
'.platform-darwin atom-text-editor':
  'ctrl-f': 'find-and-replace:find-next-selected'


# ============================================================
'.platform-darwin atom-text-editor:not([mini])':
  'ctrl-backspace': 'magic-tag:delete'

